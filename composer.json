{
    "name": "ebeyond/oauth2-fusionauth",
    "description": "Google OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Ende",
            "email": "me@e-beyond.de",
            "homepage": "https://github.com/eBeyond"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "fusionauth"
    ],
    "minimum-stability": "stable",
    "require": {
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "eBeyond\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "eBeyond\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check": "phpcs src --standard=psr2 -sp"
    }
}
